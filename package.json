{
  "name": "elm-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm i && nodemon ./bin/www | bunyan",
    "start-production": "node ./bin/www | bunyan",
    "lint": "jshint --exclude=./node_modules,./api-ui .",
    "update": "npm-check -u",
    "SEED":"------------------------------------------------------------------",
    "s-users": "node ./db/users.js",
    "s-banks": "node ./db/banks.js",
    "s-npos": "node ./db/npos.js",
    "s-roles": "node ./db/roles.js",
    "seed": "npm run s-users && npm run s-banks && npm run s-npos && npm run s-roles",
    "UI":"--------------------------------------------------------------------",
    "ui": "superstatic api-ui --port 3002 --host 127.0.0.1",
    "TEST":"------------------------------------------------------------------",
    "test": "tape tests/unit/**/*.test.js | tap-spec",
    "start-test":"DB_NAME=elm-test node ./bin/www",
    "acceptance": "tape tests/acceptance/**/*.js | tap-spec",
    "roundup": "node ./bin/roundup"
  },
  "engines": {
    "node": "5.9.0"
  },
  "dependencies": {
    "bcrypt": "0.8.5",
    "body-parser": "1.15.0",
    "bunyan": "1.8.0",
    "cors": "2.7.1",
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "hat": "0.0.3",
    "jsonwebtoken": "5.7.0",
    "mongoose": "4.4.7",
    "mongoose-timestamp": "0.5.0",
    "mongoose-unique-validator": "1.0.2"
  },
  "devDependencies": {
    "jshint": "2.9.1",
    "mockgoose": "5.3.3",
    "nodemon": "1.9.1",
    "npm-check": "4.1.4",
    "request": "2.69.0",
    "superstatic": "4.0.2",
    "tap-spec": "4.1.1",
    "tape": "4.5.1"
  }
}
